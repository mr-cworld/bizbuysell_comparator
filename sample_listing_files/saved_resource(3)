function TrackErrors(n,t,i){for(var r,u="<ClientSideErrorList>",f=0;f<n.length;f++)r=n[f],u+='<ClientSideError PageUrl="'+(i||window.location.hostname+window.location.pathname)+'" ErrorSourceName="'+t+'" ErrorTypeID="'+r.Type+'" ErrorMessage="'+r.Message+'" InputData="'+r.Input+'"><\/ClientSideError>';u+="<\/ClientSideErrorList>";TrackErrorList(u)}function TrackErrorList(n){$.ajax({url:"/xcommon/services/ErrorTracking.asmx/TraceErrorList",type:"POST",data:{xml:n}})}function PhoneFormat(n,t="undefined"){var u=$(n),i=u.val().replace(/[^0-9\.]+/g,""),f,e,r;i.length===0&&u.val("");f=!1;try{f=$(t.target).is(":-webkit-autofill")}catch(o){f=!0}if(typeof t=="undefined"||(t.keyCode<37||t.keyCode>40)&&i>0&&t.keyCode!=8&&!t.ctrlKey||f){if(e=u[0].selectionStart,r="",i.indexOf("1")===0&&(i.length==1&&u.val(""),i=i.substring(1)),i.indexOf("0")===0)return;i.length>0&&i.length<3&&(r=i.toString().replace(/(\d{1,2})/,"($1"));i.length===3&&(r=i.toString().replace(/(\d{3})/,"($1)"));i.length>3&&i.length<6&&(r=i.toString().replace(/(\d{3})(\d{1,2})/,"($1) $2"));i.length===6&&(r=i.toString().replace(/(\d{3})(\d{3})/,"($1) $2"));i.length>6&&(r=i.toString().replace(/(\d{3})(\d{3})(\d{0,4})/,"($1) $2-$3"));i.length>10&&(r=i.toString().replace(/(\d{3})(\d{3})(\d{4})(\d)/,"($1) $2-$3 x$4"));i.length>14&&(i=i.slice(0,14),r=i.toString().replace(/(\d{3})(\d{3})(\d{4})(\d)/,"($1) $2-$3 x$4"));u.val(r)}}var ClientSideErrorType={None:0,RequiredField:10,InvalidData:20,DataOutOfRange:30,DataComparisonFailure:40,UnhandledPageError:50,EmailDeliveryError:60,RequestValidation:70,PageNotFound:80},ClientSideError=function(n,t,i){this.Type=n||0;this.Input=t||"";this.Message=i||""};ClientSideError.RequiredField=function(n,t){return new ClientSideError(ClientSideErrorType.RequiredField,n.val(),t)};ClientSideError.RequiredFieldFormatError=function(n,t){var i=n.val();return new ClientSideError(i&&i.length>0?ClientSideErrorType.InvalidData:ClientSideErrorType.RequiredField,i,t)};ClientSideError.NumberFormat=function(n,t){var i=n.val();return new ClientSideError(ClientSideError.InvalidData,i,t)};$(function(){$(".formatPhone").keyup(function(n){PhoneFormat(this,n)});var n=navigator.userAgent.toLowerCase().indexOf("firefox")>-1;if(n)$(".formatPhone").on("blur input",function(){PhoneFormat(this)})})